#: ../../archive.md:6
msgid "{% for post in site.posts %}"
msgstr ""

#: ../../archive.md:8
msgid ""
"    <div class=\"ui middle aligned selection list\">\n"
"        <div class=\"item\">\n"
"            <a class=\"header\" href=\"{{ site.baseurl }}{{ post.url }}\">\n"
"                {{ post.title | strip_html | truncatewords: 15 }}\n"
"                <small class=\"small-date\">\n"
"                    {{ post.date | date_to_string }}\n"
"                    {% if post.category %} \n"
"                        in <i>{{ post.category }}</i>\n"
"                    {% endif %} \n"
"                </small>\n"
"            </a>\n"
"            {{ post.content | strip_html | truncatewords: 30 }}\n"
"        </div>\n"
"    </div>"
msgstr ""

#: ../../archive.md:23
msgid "{% endfor %}"
msgstr ""

#: ../../archive.md:25
msgid ""
"<h2>Tags</h2>\n"
"{% capture temptags %}\n"
"  {% for tag in site.tags %}\n"
"    {{ tag[1].size | plus: 1000 }}#{{ tag[0] }}#{{ tag[1].size }}\n"
"  {% endfor %}\n"
"{% endcapture %}\n"
"{% assign sortedtemptags = temptags | split:' ' | sort | reverse %}\n"
"{% for temptag in sortedtemptags %}\n"
"  {% assign tagitems = temptag | split: '#' %}\n"
"  {% capture tagname %}{{ tagitems[1] }}{% endcapture %}\n"
"  <a href=\"{{site.baseurl}}/tag/{{ tagname }}\"><code class=\"highligher-rouge\">"
"<nobr>{{ tagname }}</nobr></code></a>\n"
"{% endfor %}"
msgstr ""
